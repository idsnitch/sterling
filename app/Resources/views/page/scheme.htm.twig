{% extends 'base.htm.twig' %}
{% block banner %}{% endblock %}
  {% block breadcrumb %}
      <li class="rsmItem sfBreadcrumbNavigation">
          <div class="rsmTemplate">
              <a id="ctl00_cphBreadCrumb_T84CFA523010_ctl00_ctl00_Breadcrumb_i0_hypBreabcrumb"
                 href="{{ path('homepage') }}">Home</a>

          </div>
          <span class="sfBreadcrumbNodeSeparator"></span></li>
      <li class="rsmItem sfBreadcrumbNavigation">
          <div class="rsmTemplate">

              <a href="{{ path('services') }}" id="ctl00_cphBreadCrumb_T84CFA523010_ctl00_ctl00_Breadcrumb_i1_hypBreabcrumb">Services</a>

          </div>
      </li>
      <li><span class="sfBreadcrumbNodeSeparator"></span></li>
      <li class="rsmItem sfNoBreadcrumbNavigation">
          <div class="rsmTemplate">

              <a href="{{ path('service', {'slug': thisService.slug }) }}" id="ctl00_cphBreadCrumb_T84CFA523010_ctl00_ctl00_Breadcrumb_i1_hypBreabcrumb">{{ thisService.title }}</a>

          </div>
      </li>
      <li class="rsmItem sfNoBreadcrumbNavigation select">
          <div class="rsmTemplate">

              <a id="ctl00_cphBreadCrumb_T84CFA523010_ctl00_ctl00_Breadcrumb_i1_hypBreabcrumb">{{ scheme.scheme.title }}</a>

          </div>
      </li>

  {% endblock %}

{% block body %}

    <style>
        .bread-crumb-control{
            margin-top: 75px;
        }
    </style>
    <!-- Left column Ends -->
    <!-- Middle column  -->
    <div class="col-lg-8 col-sm-8 col-xs-12">
        <div class="main-content">


            <h1>{{ thisService }} - {{ scheme.scheme.title }}</h1>


            <div class="sfContentBlock">

                <div class="prominent-text">
                    {{ scheme.content | raw }}
                </div>
            </div>
        </div>
    </div>

    <!-- Middle column Ends -->


{% endblock %}