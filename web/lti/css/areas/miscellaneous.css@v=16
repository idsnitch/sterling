	$( document ).ready(function() {
		var checkExist = setInterval(function() {
			if ($('#habla_expanded_div').length) {
				$( "#habla_middle_div ~ div" ).css( "border", "9px solid red" );
				$( "#habla_middle_div ~ div" ).remove();
